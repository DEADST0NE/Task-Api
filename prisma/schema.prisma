generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       String @id @default(dbgenerated())
  email    String @unique
  name     String
  password String
}

model jobPositions {
  id   Int     @id
  name String?
}
